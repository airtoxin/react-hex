{
  "name": "react-hex",
  "version": "0.0.3",
  "description": "draw svg hexagonal grid with react",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "webpack",
    "watch": "webpack --watch",
    "dev": "webpack-dev-server",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint {src,test,dev,webpack*} --fix",
    "test:unit": "mocha --require jsdom-global/register --require ignore-styles --compilers jsx:babel-register --recursive test",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airtoxin/react-hex"
  },
  "keywords": [],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airtoxin/react-hex/issues"
  },
  "homepage": "https://github.com/airtoxin/react-hex#readme",
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "babel-register": "6.16.3",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "ignore-styles": "5.0.1",
    "jsdom": "9.5.0",
    "jsdom-global": "2.1.0",
    "mocha": "3.1.0",
    "random-color": "1.0.1",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
