{
  "name": "react-hex",
  "version": "0.3.3",
  "description": "draw svg hexagonal grid with react",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "NODE_ENV=production webpack",
    "watch": "webpack --watch",
    "dev": "webpack-dev-server",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint {src,test,dev,webpack*} --fix",
    "test:unit": "mocha --require jsdom-global/register --require ignore-styles --compilers jsx:babel-register --recursive test",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airtoxin/react-hex"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "hex",
    "hexagon",
    "grid",
    "svg"
  ],
  "directories": {
    "src": "src",
    "lib": "lib",
    "test": "test"
  },
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airtoxin/react-hex/issues"
  },
  "homepage": "https://github.com/airtoxin/react-hex#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "enzyme": "2.7.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "ignore-styles": "5.0.1",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "mocha": "3.2.0",
    "random-color": "1.0.1",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "babel-runtime": "6.20.0"
  }
}
