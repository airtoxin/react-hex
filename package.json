{
  "name": "react-hex",
  "version": "0.3.6",
  "description": "draw svg hexagonal grid with react",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch",
    "dev": "run-p watch storybook",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint {src,test,stories} --fix",
    "test:unit": "mocha --compilers babel-register --recursive test",
    "prepublish": "npm test && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airtoxin/react-hex"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "hex",
    "hexagon",
    "grid",
    "svg"
  ],
  "directories": {
    "src": "src",
    "lib": "lib",
    "test": "test"
  },
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airtoxin/react-hex/issues"
  },
  "homepage": "https://github.com/airtoxin/react-hex#readme",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.3.0",
    "@kadira/storybook": "2.21.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "enzyme": "2.7.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "mocha": "3.2.0",
    "npm-run-all": "3.1.2",
    "random-color": "1.0.1",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1"
  },
  "dependencies": {
    "babel-runtime": "6.20.0"
  }
}
